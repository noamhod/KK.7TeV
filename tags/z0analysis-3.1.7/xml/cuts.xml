<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE objects PUBLIC "" "cuts.dtd" []>
<objects>
<!--
  The cuts types, names and values
-->
  <obj NAME="imass" FLAG="on">
    <order>3</order>
    <type>selection</type>
    <skip>false</skip>
    <nvalues>2</nvalues>
    <values>
      <value NAME="imass_min" ORDER="1" FLAG="on">0.07</value>
      <value NAME="imass_max" ORDER="2" FLAG="on">0.4</value>
    </values>
    <description>
      This is the invariant mass cut.
      The upper boundary is for the blinded analysis.
      The values are in TeV.
    </description>
  </obj>

  <obj NAME="pT" FLAG="on">
    <order>2</order>
    <type>selection</type>
    <skip>true</skip>
    <nvalues>1</nvalues>
    <values>
      <value NAME="pT_min" ORDER="1" FLAG="on">0.02</value>
    </values>
    <description>
      This is an ordinary pT cut.
      The values are in TeV.
    </description>
  </obj>

  <obj NAME="GRL" FLAG="on">
    <order>1</order>
    <type>preselection</type>
    <skip>false</skip>
    <nvalues>1</nvalues>
    <values>
      <value NAME="isGRL" ORDER="1" FLAG="on">1</value>
    </values>
    <description>
      This is the Good Run List cut.
      The value is Boolean.
    </description>
  </obj>

</objects>
